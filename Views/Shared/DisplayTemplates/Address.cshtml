@model MvcApplication1.Models.Address

<fieldset>
    <legend>Address Info</legend>

    <table border="1" style="border-collapse:collapse">
        <tr><td>Address Line 1</td><td>@Html.DisplayFor(m => m.Line1)</td></tr>
        <tr><td>Address Line 2</td><td>@Html.DisplayFor(m => m.Line2)</td></tr>
        <tr><td>Address City</td><td>@Html.DisplayFor(m => m.City)</td></tr>
        <tr><td>Address State</td><td>@Html.DisplayFor(m => m.State)</td></tr>
        <tr><td>Address Zip</td><td>@Html.DisplayFor(m => m.Zip)</td></tr>
    </table>
</fieldset>
